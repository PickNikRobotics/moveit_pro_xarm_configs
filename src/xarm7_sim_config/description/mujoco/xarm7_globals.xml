    <position joint="joint3" name="joint3" ctrlrange="-3.14 3.14" kp="1000" forcelimited="false" dampratio="1"/>
    <position joint="joint4" name="joint4" ctrlrange="-3.14 3.14" kp="1000" forcelimited="false" dampratio="1"/>
