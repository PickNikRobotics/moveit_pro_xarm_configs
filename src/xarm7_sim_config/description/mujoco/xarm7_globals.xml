    <position joint="joint6" name="joint6" ctrlrange="-3.14 3.14" kp="1000" forcelimited="false" dampratio="1"/>
    <position joint="joint7" name="joint7" ctrlrange="-3.14 3.14" kp="1000" forcelimited="false" dampratio="1"/>
  </actuator>
</mujoco>